---
import { Image } from "astro:assets";
import Asides from "../components/Shapes/Asides.astro";
import Card from "../components/Card.astro";

import Hero from "../components/Hero.astro";
import Layout from "../layouts/Layout.astro";
import SocialMedia from "../components/SocialMedia.astro";
---

<style>
  h1,
  h2 {
    font-family: Soria, sans-serif;
  }
</style>

<Layout title="Dariels Martinez | Desarrollador de Aplicaciones Web">
  <main>
    <Hero
      description="Entusiasta de 23 años de Honduras 🇭🇳. Experiencia en proyectos digitales y comprometido con la innovación. Buscando oportunidades para crecer y colaborar en el ámbito del desarrollo web."
    />
    <h2
      class="mb-14 mt-3 pt-32 text-5xl font-serif text-black/90 sm:text-6xl text-center">
      Proyectos
